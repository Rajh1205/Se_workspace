#include <stdio.h>
#include <conio.h>

int main() {
    int num;
    int factorial = 1;   // use larger type

    printf("Enter any number: ");
    scanf("%d", &num);

    if (num < 0) {
        printf("Factorial is not defined for negative numbers.\n");
    } else {
        for (int i = 1; i <= num; i++) {
            factorial *= i;
        }
        printf("Factorial of %d = %lld\n", num, factorial);
    }

    return 0;
}
